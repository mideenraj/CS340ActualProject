
<html>
 <head>
  <meta charset="UTF-8">
  <title>Orders</title>
  <Link rel="stylesheet" href="static/css/orders_style.css" type="text/css">
 </head>
 <body>

    <!--Nav bar-->
    <div id="nav_bar">
      <ul id="nav_bar_list">
        <li id="nav_bar_elem"><a href="http://flip1.engr.oregonstate.edu:1027/">Reports</a></li>
        <li id="nav_bar_elem"><a href="http://flip1.engr.oregonstate.edu:1027/customers">Customers</a></li>
        <li id="nav_bar_elem"><a href="http://flip1.engr.oregonstate.edu:1027/orders">Orders</a></li>
        <li id="nav_bar_elem"><a href="http://flip1.engr.oregonstate.edu:1027/products">Products</a></li>
        <li id="nav_bar_elem"><a href="http://flip1.engr.oregonstate.edu:1027/departments">Departments</a></li>
        <li id="nav_bar_elem"><a href="http://flip1.engr.oregonstate.edu:1027/seasons">Seasons</a></li>
      </ul>
    </div>


    <div id="title">
        <h2>Orders</h2>
    </div>

    <div class="order_table">
      <table>

        <!-- Table Header -->
        <tr>
          <th>orderID</th>
          <th>customerID</th>
          <th>seasonID</th>
          <th>totalCost</th>
        </tr>

        <!-- Sample Data -->
        {% for row in order_data[0] %}
        <tr>
          <td>{{row['orderID']}}</td>
          <td>{{row['customerID']}}</td>
          <td>{{row['seasonID']}}</td>
          <td>{{row['totalCost']}}</td>
        </tr>
        {% endfor %}

      </table>
    </div>


 </body>
</html>